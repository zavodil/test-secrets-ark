# Keystore API configuration (direct access)
KEYSTORE_BASE_URL=http://localhost:8081

# Keystore auth token (should match KEYSTORE_AUTH_TOKEN from keystore-worker/.env)
KEYSTORE_AUTH_TOKEN=your-keystore-token-here

# NEAR contract configuration
CONTRACT_ID=offchainvm.testnet

# GitHub repository for WASM files
GITHUB_REPO=github.com/your-user/test-secrets-ark
GITHUB_BRANCH=main

# Account that will own the secrets
OWNER_ACCOUNT=owner.testnet
